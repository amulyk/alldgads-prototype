{
  "name": "alldgads-prototype",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@next/bundle-analyzer": "^9.1.5",
    "@redux-saga/core": "^1.1.3",
    "@types/express": "^4.17.2",
    "@zeit/next-sass": "^1.0.1",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "cross-env": "^6.0.3",
    "eslint-plugin-graphql": "^3.1.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql.macro": "^1.4.2",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.1.5",
    "next-compose-plugins": "^2.2.0",
    "next-i18next": "^4.1.0",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^4.0.1",
    "react": "^16.11.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node --project tsconfig.server.json ./src/server",
    "build": "yarn build:base && tsc --project tsconfig.server.json",
    "start": "cross-env-shell BUILD_ENV=development NODE_ENV=development next",
    "build:base": "next build",
    "build:dev": "cross-env-shell BUILD_ENV=development NODE_ENV=development yarn build:base",
    "build:stage": "cross-env-shell BUILD_ENV=stage NODE_ENV=production yarn build:base",
    "build:prod": "cross-env-shell BUILD_ENV=production NODE_ENV=production yarn build:base",
    "analyze": "cross-env-shell ANALYZE=true next build",
    "analyze:server": "cross-env-shell BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env-shell BUNDLE_ANALYZE=browser next build",
    "type-check": "tsc",
    "lint": "yarn type-check && yarn lint:ts && yarn lint:scss",
    "lint:ts": "eslint './src/**/*.{ts,tsx,graphql}'",
    "lint:scss": "stylelint './src/**/*.scss'",
    "prettier": "yarn prettier:ts && yarn prettier:scss",
    "prettier:ts": "prettier-eslint --list-different './src/**/*.{ts,tsx}'; prettier-eslint --write './src/**/*.{ts,tsx}'",
    "prettier:scss": "prettier-stylelint --write './src/**/*.scss'",
    "apollo:download-schema": "apollo client:download-schema --endpoint 'http://localhost:4000/'",
    "apollo:generate-types": "apollo client:codegen --localSchemaFile './schema.json' --target 'typescript'"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "24.0.20",
    "@types/next-redux-saga": "^3.0.2",
    "@types/node": "12.11.7",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/react-redux": "^7.1.5",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "apollo": "^2.21.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-plugin-graphql-tag": "^2.5.0",
    "babel-plugin-import-graphql": "^2.7.0",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "commitizen": "^4.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "next-images": "^1.3.0",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-stylelint": "^0.4.2",
    "react-scripts": "3.2.0",
    "redux-logger": "^3.0.6",
    "stylelint": "^11.1.1",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.12.1",
    "ts-node": "^8.6.2",
    "ts-transform-graphql-tag": "^0.2.1",
    "typed-redux-saga": "^0.4.0",
    "typescript": "3.6.4"
  }
}
